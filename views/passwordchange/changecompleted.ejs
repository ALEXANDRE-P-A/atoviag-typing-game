<!DOCTYPE html>
<html lang="ja">
<head>
  <%- include("../_share/metadata.ejs") %>
  <title>Password Updated </title>
  <%- include("../_share/stylesheets.ejs") %>
</head>
<body>
  <%- include("../_share/header.ejs") %>
  <div class="container">
    <div class="border-bottom my-5">
      <h1 class="">Check your E-mail Box</h1>
    </div>
    <div class="alert alert-success my-5 text-center" style="padding: 6px 12px;">Password for the email account <%= email %> successfully at <%= moment(date).add(540, "minutes").format('MMMM Do YYYY, h:mm:ss a') %></div>
    <div class="text-center my-5">
      <a class="btn btn-outline-primary" href="/">Top Page</a>
    </div>
  </div>
  <%- include("../_share/footer.ejs") %>
  <%- include("../_share/javascripts.ejs") %>
  <!-- browserback prevention -->
  <script type="text/javascript" src="/public/js/browserbackprevention.js"></script>
</body>
</html>